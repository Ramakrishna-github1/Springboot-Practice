pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
    }

    stages {
        stage('Build') {
            steps {
               git branch: 'dev', url: 'https://github.com/Ramakrishna-github1/Springboot-Practice.git'
               
               bat "mvn -Dmaven.test.failure.ignore=true clean package"
        }
    }
}
}
